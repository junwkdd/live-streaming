{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "script",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "env": {
      "browser": true,
      "node": true
  },
  "rules": {
      "prettier/prettier": "error"
  }
}
